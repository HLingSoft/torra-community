{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "lib": ["DOM", "ESNext"],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      
    },
    "typeRoots": ["./node_modules/@types"],
    "types": ["node", "nuxt"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "preserveConstEnums": true
  }
}
